<%-- any content can be specified here e.g.: --%>
<%@page import="Services.Slides"%>
<%@ page pageEncoding="UTF-8" %>
<table class="none">
    <tr>
        <td class="none">
            <h6><%= new Slides().getSlides(6, 6)%></h6>
            <br>
        </td>
    </tr>
    <%for (int t=7;t<11;t++){%>
    <tr>
        <td class="none">
            <h6 class="justify"> <%= t-6+"- "+new Slides().getSlides(6, t)%></h6>
        </td>
    </tr>
    <%}%>
</table> 
